<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="243.0" prefWidth="277.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ejercicio1di.MainViewController">
   <center>
      <VBox prefHeight="147.0" prefWidth="238.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btn" alignment="CENTER" mnemonicParsing="false" onAction="#mostrar" text="Button" />
            <ChoiceBox fx:id="choice" nodeOrientation="LEFT_TO_RIGHT" prefHeight="34.0" prefWidth="135.0" />
            <DatePicker fx:id="fecha" />
            <Spinner fx:id="spinner" />
            <ListView fx:id="lista" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#volver" prefHeight="42.0" prefWidth="66.0" text="Volver" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
